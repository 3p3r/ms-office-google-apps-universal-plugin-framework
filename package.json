{
  "name": "ms-office-google-apps-universal-plugin-framework",
  "version": "1.0.0",
  "description": "A Universal framework to develop React based add-ins for both MS Office and its Google alternatives",
  "main": "index.js",
  "scripts": {
    "install:google": "yarn --cwd google install",
    "install:office": "npm install --prefix office",
    "install": "npm-run-all --parallel install:google install:office"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3p3r/ms-office-google-apps-universal-plugin-framework.git"
  },
  "keywords": [
    "word",
    "excel",
    "powerpoint",
    "google docs",
    "google sheets",
    "google slides",
    "office add-ins",
    "google apps script",
    "react"
  ],
  "author": "Sepehr Laal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3p3r/ms-office-google-apps-universal-plugin-framework/issues"
  },
  "homepage": "https://github.com/3p3r/ms-office-google-apps-universal-plugin-framework#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
