{
  "name": "ms-office-google-apps-universal-plugin-framework",
  "version": "1.0.0",
  "description": "A Universal framework to develop React based add-ins for both MS Office and its Google alternatives",
  "main": "index.js",
  "scripts": {
    "install:google": "yarn --cwd google install",
    "install:office": "npm install --prefix office",
    "install": "npm-run-all --parallel install:google install:office",
    "build:google": "yarn --cwd google build",
    "build:office": "npm run build --prefix office",
    "build": "npm-run-all --parallel build:google build:office",
    "start:google": "yarn --cwd google run start",
    "start:office": "npm run dev-server --prefix office"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3p3r/ms-office-google-apps-universal-plugin-framework.git"
  },
  "keywords": [
    "word",
    "excel",
    "powerpoint",
    "google docs",
    "google sheets",
    "google slides",
    "office add-ins",
    "google apps script",
    "react"
  ],
  "author": "Sepehr Laal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3p3r/ms-office-google-apps-universal-plugin-framework/issues"
  },
  "homepage": "https://github.com/3p3r/ms-office-google-apps-universal-plugin-framework#readme",
  "dependencies": {
    "core-js": "^3.46.0",
    "es6-promise": "^4.2.8",
    "gas-client": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2"
  }
}
